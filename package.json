    "eslint-plugin-react": "^7.26.1",
    "husky": "^7.0.0",
    "less": "^4.1.2",
    "postcss": "^8.3.9",
    "prettier": "^2.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "tailwindcss": "^2.2.16",
    "typescript": "^4.4.3",
    "vite": "^2.6.4"
